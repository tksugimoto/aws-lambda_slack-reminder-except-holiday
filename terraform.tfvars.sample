# Rename: terraform.tfvars.sample → terraform.tfvars

# AWSのアクセスキー
aws_access_key = "AKIA_____________________"
aws_secret_key = "_______________________________"

# 各種リソースの名前のprefix
prefix = "Slack_Reminder"

# リージョンを東京に設定
region = "ap-northeast-1"

# Slack WebhookのURL
# 一括設定したい場合用
slack_webhook_url = "https://hooks.slack.com/services/*********/*********/************************"

# 休みの追加（[,]区切り）
additional_holidays = "12/29-31, 1/2, 1/3"
